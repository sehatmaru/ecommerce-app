<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="InputDataWithLabelViewKG">
        <attr name="titleInputGeneral" format="reference"/>
        <attr name="hintInputGeneral" format="reference"/>
        <attr name="imgInputGeneral" format="reference"/>
        <attr name="maxInputGeneral" format="integer"/>
        <attr name="formatTypeGeneral" format="enum">
            <enum name="text" value="0"/>
            <enum name="number" value="1"/>
            <enum name="phone" value="2"/>
            <enum name="email" value="3"/>
        </attr>
        <attr name="enableTypeOne" format="boolean"/>
        <attr name="showTitle" format="boolean"/>
        <attr name="withClear" format="boolean"/>
        <attr name="isPassword" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="ListDataProfileViewKG">
        <attr name="titleProfile" format="reference"/>
        <attr name="hintProfile" format="reference"/>
    </declare-styleable>

    <declare-styleable name="ListCategoryShopViewKG">
        <attr name="categoryShop" format="enum">
            <enum name="news" value="0"/>
            <enum name="clothes" value="1"/>
            <enum name="shoes" value="2"/>
            <enum name="accesories" value="3"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="ListItemBagViewKG">
        <attr name="itemCat" format="enum">
            <enum name="first" value="0"/>
            <enum name="second" value="1"/>
            <enum name="third" value="2"/>
        </attr>
        <attr name="itemSize" format="enum">
            <enum name="s" value="0"/>
            <enum name="m" value="1"/>
            <enum name="l" value="2"/>
            <enum name="xl" value="3"/>
        </attr>
        <attr name="itemColor" format="enum">
            <enum name="black" value="0"/>
            <enum name="navy" value="1"/>
            <enum name="red" value="2"/>
            <enum name="green" value="3"/>
        </attr>
    </declare-styleable>
</resources>
